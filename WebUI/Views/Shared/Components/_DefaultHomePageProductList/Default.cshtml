@model List<WebUI.DTOs.Product.ResultProductDto>
<section class="locations-1" id="locations">
    <div class="locations py-5">
        <div class="container py-lg-5 py-md-4 py-2">
            <div class="heading text-center mx-auto">
                <h3 class="title-big">Favori İlanlar</h3>
            </div>
                <div class="row pt-md-5 pt-4">
                @foreach (var product in Model)
                {
                    <div class="col-lg-4 col-md-6 mb-3">
                        <a href="property-single.html">
                            <div class="box16">
                                <div class="rentext-listing-category"><span>@product.type</span><span>@product.name</span></div>
                                <img class="img-fluid" src="@product.coverImage" alt="">
                                <div class="box-content">
                                    <h3 class="title">@product.price</h3>
                                    <span class="post">@product.address @product.district - @product.city</span>
                                </div>
                            </div>
                           
                        </a>
                    </div>
            }
            </div>

        </div>
    </div>
</section>